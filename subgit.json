			"hash": "bb16bc9a1b386511ef8b56c0b4e2934e6283375c39bf7ad99bc56d39c1c7e317"
        }
    },
	"extract_dir": "subgit-3.3.3",
    "env_add_path": "bin",
    "checkver": {
        "url": "https://subgit.com/download",
        "re": "(?<version>\\d\\.\\d\\.\\d)/ #(?<build>[\\d]+)/ of (?<date>[u\\d\\.]+)",
        "reverse": false
    },
    "autoupdate": {
		"note": "This scoop install manifest was created by Brett Brewer. If you encounter issues please post an issue on the github page.",
        "architecture": {
            "64bit": {
                "url": "https://subgit.com/files/subgit-$version.zip#/dl.7z",
				"extract_dir": "subgit-$version"
            },
            "32bit": {
                "url": "https://subgit.com/files/subgit-$version.zip#/dl.7z",
				"extract_dir": "subgit-$version"
            }
        }
      
    },
	"persist": [
		"daemon-shared"
	],
	"pre_install": [
		"$root=(scoop which subgit | split-path -res -par | split-path -par)",
		"Write-Host \"To uninstall service type:\n\"",
		"Write-Host \"#sc.exe stop \"SubgitService\"\"\n",
		"Write-Host \"#sc.exe delete \"SubgitService\"\"\n"
	],
	"post_install": [
		"$root=(scoop which subgit | split-path -res -par | split-path -par)",
		"Write-Host \"To install service type:\n\"",
		"Write-Host \"#sc.exe create \"SubgitService\" type= own start= auto error= normal binpath= \"$root\\bin\\subgit.bat daemon start $root\\daemon-shared\" displayname= \"Subgit Service\"\"\n",
		"Write-Host \"#sc.exe start \"SubgitService\"\"\n"
	],
	"notes":[
		"Yay."
	]
}
